<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane prefHeight="240.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafxwelcome.LoginController">
    <stylesheets>
        <URL value="@style.css" />
    </stylesheets>
   <children>
      <Label layoutX="15.0" layoutY="14.0" styleClass="welcomeLabel" text="Welcome" />
      <Label layoutX="32.0" layoutY="75.0" text="Username:" />
      <Label layoutX="39.0" layoutY="112.0" text="Password:" />
      <TextField fx:id="usernameInput" layoutX="106.0" layoutY="71.0" />
      <PasswordField fx:id="passwordInput" layoutX="106.0" layoutY="108.0" />
      <Button layoutX="202.0" layoutY="148.0" mnemonicParsing="false" onMouseClicked="#login" text="Log in" />
      <Label fx:id="statusLabel" layoutX="146.0" layoutY="133.0" />
   </children>

</Pane>
