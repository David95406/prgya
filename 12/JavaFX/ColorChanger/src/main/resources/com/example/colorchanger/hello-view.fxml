<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>

<Pane prefHeight="400.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.colorchanger.HelloController">
    <children>
        <Circle fx:id="kor" fill="#1f93ff" layoutX="114.0" layoutY="106.0" onMouseClicked="#changeCircleColor"
                onMouseEntered="#removeCircleStroke" onMouseExited="#resetCircleStroke" radius="100.0" stroke="BLACK"
                strokeType="INSIDE" strokeWidth="2.0"/>
        <Ellipse fx:id="ellipszis" fill="DODGERBLUE" layoutX="228.0" layoutY="279.0"
                 onMouseClicked="#changeEllipseColor" onMouseEntered="#removeEllipseStroke"
                 onMouseExited="#resetEllipseStroke" radiusX="100.0" radiusY="73.0" stroke="BLACK" strokeType="INSIDE"
                 strokeWidth="2.0"/>
        <Rectangle fx:id="negyzet" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="116.0" layoutX="228.0"
                   layoutY="66.0" onMouseClicked="#changeRectangleColor" onMouseEntered="#removeRectangleStroke"
                   onMouseExited="#resetRectangleStroke" stroke="BLACK" strokeType="INSIDE" strokeWidth="2.0"
                   width="126.0"/>
    </children>
</Pane>
