        <URL value="@style.css" />
    </stylesheets>
    <children>
        <Label layoutX="14.0" layoutY="14.0" styleClass="cimLabel" text="Főmenü" />
        <Button onMouseClicked="#createEvent" layoutX="30.0" layoutY="58.0" mnemonicParsing="false" prefHeight="250.0" prefWidth="140.0" text="Létrehozás" />
        <Button onMouseClicked="#listEvent" layoutX="180.0" layoutY="58.0" mnemonicParsing="false" prefHeight="250.0" prefWidth="140.0" text="Lista" />
        <Button onMouseClicked="#seachEvent" layoutX="330.0" layoutY="58.0" mnemonicParsing="false" prefHeight="250.0" prefWidth="140.0" text="Keresés" />
    </children>


</Pane>
